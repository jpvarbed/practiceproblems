{
  "compilerOptions": {
    "strict": true,
    "useUnknownInCatchVariables": false,
    "sourceMap": true,
    "module": "ESNext",
    "target": "ESNext",
    "lib": [
      "es2021",
      "DOM"
    ],
    "jsx": "preserve",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "allowJs": true,
    "outDir": "./output",
  },
}